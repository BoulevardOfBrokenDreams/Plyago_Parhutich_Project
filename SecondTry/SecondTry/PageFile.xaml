<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SecondTry.PageFile">
    <StackLayout>
        <StackLayout>
            <Label x:Name="DayOfWeek"></Label>
        </StackLayout>
        <StackLayout>
            <Label x:Name="first"></Label>
            <Label x:Name="second"></Label>
            <Label x:Name="third"></Label>
            <Label x:Name="fourth"></Label>
        </StackLayout>
        <StackLayout>
            <Button x:Name="backButton" Clicked="BackButtonClick"></Button>
        </StackLayout>
    </StackLayout>
</ContentPage>